{
    "name": "toggle-boolean",
    "displayName": "Toggle Boolean",
    "description": "Quickly toggle boolean values. Inspired by the vim plugin, toggle-bool.",
    "icon": "images/icon_128.png",
    "version": "0.9.1",
    "publisher": "silesky",
    "engines": {
        "vscode": "^1.17.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/silesky/vscode-toggle-bool.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.toggleBool"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "extension.toggleBool",
                "key": "alt+b",
                "when": "editorTextFocus"
            }
        ],
        "commands": [
            {
                "command": "extension.toggleBool",
                "title": "Toggle Boolean"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "lint": "eslint src/**/*.ts",
        "fix": "npm run lint --fix",
        "upgrade-interactive": "npm-check --update"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.0",
        "@types/node": "^14.14.25",
        "@typescript-eslint/eslint-plugin": "^4.14.2",
        "@typescript-eslint/parser": "^4.14.2",
        "eslint": "^7.19.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "npm-check": "^5.9.2",
        "prettier": "^2.1.2",
        "typescript": "^4.1.3",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "npm": "^7.5.2"
    }
}
